{
  "root": "packages/directives",
  "sourceRoot": "packages/directives/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/directives/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/directives"],
      "options": {
        "jestConfig": "packages/directives/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
