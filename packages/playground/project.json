{
  "root": "packages/playground",
  "projectType": "application",
  "sourceRoot": "packages/playground/src",
  "targets": {
    "build": {
      "executor": "nx-vue3-vite:build-app",
      "options": {
        "dist": "dist/packages/playground"
      }
    },
    "serve": {
      "executor": "nx-vue3-vite:dev-server"
    },
    "e2e": {
      "executor": "nx-vue3-vite:cypress",
      "options": {
        "cypressConfig": "packages/playground/cypress.json",
        "testingType": "component"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/playground/**/*.{js,jsx,ts,tsx,vue}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/playground"],
      "options": {
        "jestConfig": "packages/playground/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
